<h1>Listing articles</h1>

<table>
  <tr><th></th>
    <th>Title</th>
    <th>Image url</th>
    <th>Artist</th>
    <th>Intro</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @articles.each do |article| %>
  <tr>
  	 <td><a href="/articles/<%= article.id %>"> show</a></td>
    <td><%= article.title %></td>
    <td><%= article.image_url %></td>
    <td><%= article.artist %></td>
    <td><%= article.intro %></td>
    <td><%= article.description %></td>
   <td><!--<%# link_to 'Show', article_path %>--></td>

  </tr>
<% end %>
</table>

